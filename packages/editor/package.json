{
  "private": true,
  "name": "editor",
  "version": "1.0.0",
  "license": "GPL",
  "scripts": {
    "start": "chmod +x ./scripts/env.sh && ./scripts/env.sh && node scripts/start.js",
    "build": "sh -ac 'ORIGIN=ethereum.org node scripts/build.js'",
    "build:clean": "rimraf dist",
    "test": "mocha ./test/*.js",
    "test:watch": "npm test -- --watch",
    "format": "prettier --trailing-comma es5 --single-quote --write 'src/*/*.js' 'src/*/!(node_modules)/**/*.js' '!src/services/solc/**/*.js' '!src/services/evm/**/*.js' webpack.config.js",
    "lint-javascript": "exec eslint --config ./config/eslint/eslint.json --ext \".js\" ./src",
    "lint": "yarn run lint-javascript",
    "postinstall": "node scripts/postinstall.js && node scripts/generate-templates.js",
    "generatetemplates": "node scripts/generate-templates.js",
    "inject-env-variables": "chmod +x scripts/env.sh && scripts/env.sh && cp public/env-config.js ../../www"
  },
  "eslintIgnore": [
    "ethereumjs-tx-*.js",
    "src/services/evm/dist/*",
    "src/services/solc/dist/*",
    "src/components/superProvider/dist/*"
  ],
